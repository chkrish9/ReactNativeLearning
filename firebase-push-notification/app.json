      "package": "com.chkrish9.firebasepushnotification"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6cc18dd6-ff22-4372-accb-0b8282ebe1c0"
